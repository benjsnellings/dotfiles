
set tabstop=4
set softtabstop=4
set expandtab
set number
set showcmd
set cursorline

" Search Settings
set ignorecase
set smartcase


" Key Bindings -------------------------------------------------------------------------
let mapleader = ","

inoremap jk <esc>

xnoremap <leader>p "_dP

noremap <C-p> viw"_dP






