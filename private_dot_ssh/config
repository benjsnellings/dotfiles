  User snellin


# The following config is added by WSSH setup script to configure off-VPN Git access.
Host git.amazon.com
  ProxyCommand=/usr/local/bin/wssh proxy %h --port=%p

Host github.audible.com
  ProxyCommand=/usr/local/bin/wssh proxy %h --port=%p


