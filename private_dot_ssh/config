# Enable Kerb Forwarding
Host git.amazon.com
	GSSAPIAuthentication yes
	GSSAPIDelegateCredentials yes



# Host DevDesktop
    # HostName dev-dsk-snellin-2b-8d3a360b.us-west-2.amazon.com
    # User snellin

Host DevDesktop
  HostName  snellin-dsk.aka.corp.amazon.com
        # HostName dev-dsk-snellin-2b-8d3a360b.us-west-2.amazon.com 
  ProxyCommand=/usr/local/bin/wssh proxy %h
  ServerAliveInterval 15
  ServerAliveCountMax 44
  ForwardAgent yes
  User snellin

Host XlDevDesktop
  HostName  dev-dsk-snellin-2b-m7a-24-e0688e64.us-west-2.amazon.com
  ProxyCommand=/usr/local/bin/wssh proxy %h
  ServerAliveInterval 15
  ServerAliveCountMax 44
  ForwardAgent yes
  User snellin


Host AxeDesktop
  ProxyCommand axe connect --instance-id "i-0bb1a1620bea6217f" --proxy --port %p
  ServerAliveInterval 15
  ServerAliveCountMax 44
